generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("LOCAL_DATABASE_URL")
}

model Coords {
  id        String @id @default(uuid())
  character String
  xMin      Int
  xMax      Int
  yMin      Int
  yMax      Int
}
